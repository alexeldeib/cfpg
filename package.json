{
  "name": "cfpg",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "diff": "prisma migrate diff --from-schema-datasource ./prisma/schema.prisma --to-schema-datamodel ./prisma/schema.prisma --script",
    "migrate": "prisma migrate dev",
    "gen": "prisma generate",
    "freset": "npx prisma migrate reset --skip-generate --skip-seed -f",
    "pages:build": "npx @cloudflare/next-on-pages@1",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "pages:watch": "npx @cloudflare/next-on-pages@1 --watch",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./lib/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@simplewebauthn/browser": "^8.2.1",
    "@simplewebauthn/server": "^8.2.0",
    "@simplewebauthn/typescript-types": "^8.0.0",
    "@tailwindcss/forms": "^0.5.6",
    "@types/cookie": "^0.5.2",
    "@types/node": "20.7.1",
    "@types/pg": "^8.10.3",
    "@types/react": "18.2.23",
    "@types/react-dom": "18.2.8",
    "autoprefixer": "10.4.16",
    "cookie": "^0.5.0",
    "eslint": "8.50.0",
    "eslint-config-next": "13.5.3",
    "iron-session": "github:renchris/iron-session#v8-as-dependency",
    "iron-webcrypto": "^0.10.1",
    "kysely": "^0.26.3",
    "next": "13.5.3",
    "pg": "^8.11.3",
    "postcss": "8.4.30",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.2.4",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.6.1",
    "@storybook/addon-essentials": "^7.4.5",
    "@storybook/addon-interactions": "^7.4.5",
    "@storybook/addon-links": "^7.4.5",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.5",
    "@storybook/nextjs": "^7.4.5",
    "@storybook/react": "^7.4.5",
    "@storybook/testing-library": "^0.2.1",
    "eslint-plugin-next-on-pages": "^1.6.1",
    "eslint-plugin-storybook": "^0.6.14",
    "prisma": "^5.3.1",
    "prisma-kysely": "^1.7.1",
    "storybook": "^7.4.5",
    "ts-node": "^10.9.1",
    "vercel": "^32.3.1"
  }
}
